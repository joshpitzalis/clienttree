  "scripts": {
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run --record --key a7efa281-ee2d-4ee8-9ab3-158226603f23",
    "cypress:ci": "START_SERVER_AND_TEST_INSECURE=1  start-server-and-test start http-get://localhost:3000 cypress:run",
    "cypress": "start-server-and-test start http://localhost:3000 cypress:open",
    "deploy": "firebase deploy --except functions",
    "functions": "firebase deploy --only functions",
    "coverage": "npm test -- --coverage",
    "build:tailwind": "postcss src/tailwind.css -o src/tailwind.generated.css",
    "watch:tailwind": "postcss -w src/tailwind.css -o src/tailwind.generated.css",
    "start": "run-p watch:tailwind start:react",
    "start:react": "node scripts/start.js",
    "prebuild": "npm run build:tailwind",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "docz": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:react/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "eslint-plugin-cypress",
      "jest",
      "jsx-a11y",
      "react",
      "react-hooks"
    ],
    "env": {
      "cypress/globals": true
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.3.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@xstate/test": "^0.1.0",
    "babel-eslint": "^9.0.0",
    "canvas": "^2.6.1",
    "cypress": "^4.5.0",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.1",
